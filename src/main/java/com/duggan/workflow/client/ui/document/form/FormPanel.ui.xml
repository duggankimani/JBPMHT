<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v='urn:import:com.duggan.workflow.client.ui.component'>
	<g:HTMLPanel>
		<v:IssuesPanel ui:field="issues">
		</v:IssuesPanel>
		<div class="row-fluid item">
			<div class="10">
				<legend ui:field="divFormCaption">
				</legend>
			</div>
			<div class="span2">
				<!-- <i class="icon-calendar icon-large"></i> -->
				<div style="float:right; text-align:right">
					<span ui:field="spnCreated" class="tasktimelines">June 21st 2014</span>
					<span ui:field="spnDeadline" class="tasktimelines">Overdue</span>
				</div>

			</div>
		</div>
		<div class="row-fluid item">
			<span ui:field="divFormHelp" styleName="text-info"></span>
		</div>
		<g:HTMLPanel styleName="form-horizontal" ui:field="panelFields">
			<div class="control-group">
				<strong>
					<g:InlineLabel styleName="control-label" ui:field="panelLabel">
					</g:InlineLabel>
				</strong>
				<g:HTMLPanel styleName="controls" ui:field="panelItem"></g:HTMLPanel>
			</div>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>