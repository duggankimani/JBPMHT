<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

		<!-- Item 1 -->
		<g:HTMLPanel styleName="item" ui:field="divWrapper">
			<div class="row">
				<div class="col-md-12 text-center">
					<g:Image ui:field="imgScreenshot"/>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<strong>
						<p class="color-silver-dark helper-font-12" ui:field="divText">Click on the Add Document Button. Choose the Type of
							document
						</p>
					</strong>
				</div>
			</div>
		</g:HTMLPanel>

</ui:UiBinder> 