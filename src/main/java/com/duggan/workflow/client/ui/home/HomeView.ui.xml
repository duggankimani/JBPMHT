<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	xmlns:d='urn:import:com.duggan.workflow.client.ui.component' xmlns:tab='urn:import:com.duggan.workflow.client.ui.home'
	ui:generateLocales='default'>

	<g:HTMLPanel ui:field="mainContainer" styleName="container-fluid">
		<div class="row">
			<div class="col-md-8">.col-md-8</div>
			<div class="col-md-4">.col-md-4</div>
		</div>
		<div class="row">
			<div id="sidebar-nav" class="col-md-2 col-lg-2 hidden-phone">

				<!--Button Add Document -->
				<div class="add-doc">
					<g:Anchor ui:field="btnAdd"
						styleName="btn btn-danger btn-large dropdown-toggle">
						Start Process
					</g:Anchor>

					<!-- Document Pop-up -->
					<g:HTMLPanel ui:field="divDocPopup" styleName="dropdown-menu document-drop">
					</g:HTMLPanel>

				</div>

				<g:HTMLPanel styleName="container-fluid">
					<ul id="dashboard-menu">
						<li class="active">
							<a class="dropdown-toggle" href="#home;type=appreqnew">
								<i class="icon-tasks"></i>
								<span>My Tasks</span>
								<i class="icon-chevron-up hidden-tablet"></i>
							</a>
							<tab:HomeTabPanel ui:field="tabPanel"></tab:HomeTabPanel>
						</li>

						<li class="active">
							<a class="dropdown-toggle" href="#home">
								<i class="icon-book"></i>
								<span>My Documents</span>
								<i class="icon-chevron-up hidden-tablet"></i>
							</a>
							<g:HTMLPanel ui:field="panelDocTree">

							</g:HTMLPanel>
						</li>
					</ul>
				</g:HTMLPanel>
			</div>

			<g:HTMLPanel ui:field="tabContent" styleName="col-md-10">
			</g:HTMLPanel>
		</div>
	</g:HTMLPanel>



</ui:UiBinder>