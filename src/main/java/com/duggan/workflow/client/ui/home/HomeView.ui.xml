<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	xmlns:d='urn:import:com.duggan.workflow.client.ui.component' xmlns:tab='urn:import:com.duggan.workflow.client.ui.home'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="row-fluid main-container">
		<g:HTMLPanel ui:field="mainContainer" styleName="full-page">
			<div id="sidebar-nav" class="span2">

				<!--Button Add Document -->
				<div class="add-doc">
					<g:Anchor ui:field="btnAdd" styleName="btn-danger btn-large dropdown-toggle">
						Start Process
					</g:Anchor>

					<!-- Document Pop-up -->
					<g:HTMLPanel ui:field="divDocPopup" styleName="dropdown-menu document-drop">
					</g:HTMLPanel>

				</div>

				<g:HTMLPanel styleName="full-page">
					<ul id="dashboard-menu">
						<li>
							<ul class="documents submenu" ui:field="sideBarUL">
								<div class="accordion" id="accordion2">
									<div class="accordion-group">
										<div class="accordion-heading">
											<i class="icon-inbox"></i>
											<a id="inbox" class="accordion-toggle" data-toggle="collapse"
												data-parent="#accordion2" href="#collapseOne">
												Inbox (1)
											</a>
											<i class="icon-caret-right pull-right"
												style="margin-right: 10px;margin-top: 8px;color: #ADA5A5;"></i>
										</div>
										<div id="collapseOne" class="accordion-body collapse">
											<ul class="submenu">
												<li class="accordion-inner">
													<a href="#/inbox/mine">Mine (1)</a>
												</li>
												<li class="accordion-inner">
													<a href="#/inbox/queued">Queued (0)</a>
												</li>
												<li class="accordion-inner">
													<a href="#/inbox/all">All (0)</a>
												</li>
											</ul>
										</div>
									</div>

									<div class="accordion-group">
										<div class="accordion-heading">
											<i class="icon-pencil"></i>
											<a class="accordion-toggle" href="#/drafts">
												Drafts (26)
											</a>
										</div>
									</div>
									<div class="accordion-group">
										<div class="accordion-heading">
											<i class="icon-time"></i>
											<a class="accordion-toggle" href="#/participated">Participated (29)</a>
										</div>
									</div>
									<div class="accordion-group">
										<div class="accordion-heading">
											<i class="icon-flag"></i>
											<a class="accordion-toggle" href="#/suspended">Suspended (5)</a>
										</div>
									</div>
									<div class="accordion-group">
										<div class="accordion-heading">
											<i class="icon-warning-sign"></i>
											<a class="accordion-toggle" href="#/unassigned">UnAssigned (1)</a>
										</div>
									</div>
									<div class="accordion-group">
										<div class="accordion-heading">
											<i class="icon-archive"></i>
											<a class="accordion-toggle" href="#/registry">Case Registry</a>
										</div>
									</div>
									<div class="accordion-group">
										<div class="accordion-heading">
											<i class="icon-file"></i>
											<a class="accordion-toggle" href="#/reports" style="padding-left:14px;">Reports</a>
										</div>
									</div>
									<div class="accordion-group">
										<div class="accordion-heading">
											<i class="icon-folder-open"></i>
											<a class="accordion-toggle" href="#/explorer">File Explorer</a>
										</div>
									</div>
								</div>
							</ul>
						</li>





						<li class="active hide">
							<a class="dropdown-toggle" href="#/inbox">
								<i class="icon-tasks"></i>
								<span>My Tasks</span>
								<i class="icon-chevron-up hidden-tablet"></i>
							</a>
							<tab:HomeTabPanel ui:field="tabPanel"></tab:HomeTabPanel>
						</li>

						<li class="active hide">
							<!-- a class="dropdown-toggle hide" href="#/home"> <i class="icon-book"></i> 
								<span>My Documents</span> <i class="icon-chevron-up hidden-tablet"></i> </a -->
							<legend></legend>
							<ul class="documents">
								<li>
									<i class="icon-archive"></i>
									<a href="#/registry" ui:field="aCaseReg">Case Registry</a>
								</li>
								<li>
									<i class="icon-file-alt"></i>
									<a href="#/reports" ui:field="aReports">Report Registry</a>
								</li>
								<li>
									<i class="icon-folder-open-alt"></i>
									<a href="#/explorer" ui:field="aExplorer">File Explorer</a>
								</li>

							</ul>
							<g:HTMLPanel ui:field="panelDocTree" height="500px">
							</g:HTMLPanel>
						</li>
					</ul>
				</g:HTMLPanel>
			</div>

			<g:HTMLPanel ui:field="tabContent" styleName="content-right">
			</g:HTMLPanel>
		</g:HTMLPanel>

	</g:HTMLPanel><!--End of Container -->


</ui:UiBinder>