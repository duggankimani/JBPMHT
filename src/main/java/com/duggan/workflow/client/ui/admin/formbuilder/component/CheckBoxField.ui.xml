<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:AbsolutePanel ui:field="container">
		<g:HTMLPanel styleName="control-group" ui:field="panelGroup">
			<label ui:field="lblEl" class="control-label">Checkbox</label>
			<div class="controls">
				<g:CheckBox ui:field="component"></g:CheckBox>
				<span ui:field="spnMsg" class="hide help-inline">
					<i class="icon" ui:field="spnIcon"></i>
				</span>
				<span ui:field="spnMandatory" style="color:red">*</span>
			</div>
		</g:HTMLPanel>
	</g:AbsolutePanel>
</ui:UiBinder> 