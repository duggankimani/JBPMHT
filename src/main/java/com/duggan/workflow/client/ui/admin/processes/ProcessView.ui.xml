<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:d='urn:import:com.duggan.workflow.client.ui.component'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
				<div class="content-body overflow-y">
					<div class="row-fluid">
						<div class="action-buttons">
							<g:Anchor ui:field="aNewProcess" styleName="btn btn-primary"
								title="Add a New Process">
								<i class="icon-plus"></i>
								Add Process
							</g:Anchor>
							<g:Anchor ui:field="aStartProcesses" styleName="btn btn-success"
								title="Start All Processes">
								<i class="icon-ok"></i>
								Start All Processes
							</g:Anchor>
							<g:Anchor ui:field="aAddCategory" styleName="btn btn-primary"
								title="Add Process Category">
								<i class="icon-plus"></i>
								Add/Edit Categories
							</g:Anchor>
					   </div>																			
					</div>
					
					<div class="table table-striped table-hover table-bordered table-process">
					    <div class="thead">
					    	<div class="tr">
								<div class="th">
								  <label><input type="checkbox" class="ace"/></label>
								</div>
								<div class="th" style="">NAME:</div>
								<div class="th" style="">CATEGORY</div>
								<div class="th" style="">FILE NAME</div>
								<div class="th" style="">IMAGE</div>
								<div class="th" style="">PROCESS ID</div>
								<div class="th" style="">STATUS</div>
								<div class="th" style="">Active</div>
								<div class="th" style="">Completed</div>
								<div class="th" style="">Total</div>
								<div class="th" style="">MODIFIED</div>
								<div class="th" style="width:158px;">ACTIONS</div>	
							</div>								

						</div>
						<g:HTMLPanel ui:field="tblRow" styleName="tbody">
						</g:HTMLPanel>
					</div>
				</div>
		</g:HTMLPanel>
</ui:UiBinder>
