<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:com.duggan.workflow.client.ui.component">
	<g:FocusPanel ui:field="parentPanel">
	<g:HTMLPanel>
		<g:FocusPanel ui:field="focusContainer">
		</g:FocusPanel>

		<g:HTMLPanel ui:field="innerContainer">
			<div class="carousel">
				<div class="row">
					<div class='offset-2 col-md-11 text-center'>
						<h2>
							<span ui:field="spnHeader">Creating New Request</span>
						</h2>
					</div>
					<g:Anchor ui:field="aClose" styleName="helper-font-16 color-silver-dark">
						<span class="glyphicon glyphicon-remove-sign pull-right"></span>
					</g:Anchor>
					
				</div>
				<div class='row'>
					<div class='offset-2 col-md-12'>
						<div class="carousel" data-ride="carousel" data-interval="false"
							id="quote-carousel">
							<!-- Bottom Indicators -->
							<p:OLPanel styleName="carousel-indicators" ui:field="olCarousels">
							</p:OLPanel>

							<!-- Inner Items Carousel -->
							<g:HTMLPanel styleName="carousel-inner" ui:field="innerCarousel">
							</g:HTMLPanel>


							<!-- Carousel Buttons Next/Prev -->
							<g:HTMLPanel ui:field="panelControls">
								<a data-slide="prev" href="#quote-carousel" class="left carousel-control">
									<i class="glyphicon glyphicon-chevron-left"></i>
								</a>
								<a data-slide="next" href="#quote-carousel" class="right carousel-control">
									<i class="glyphicon glyphicon-chevron-right"></i>
								</a>
							</g:HTMLPanel>
						</div>
					</div>
				</div>
			</div>
		</g:HTMLPanel>
	</g:HTMLPanel>
	</g:FocusPanel>
</ui:UiBinder> 