<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:up='urn:import:com.duggan.workflow.client.ui.upload.custom' xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:DialogBox modal="true"
	    width="1000px" height="600px" ui:field="uploaderDialog" text="Request For Quotations- RFQ/7891/13">
		<g:HTMLPanel>
				<iframe src="http://localhost:8080" ui:field="iframe" width="1000px" height="600px">
				    
				</iframe>
			<g:HTMLPanel styleName="modal-footer">
				<g:Button styleName="btn btn-primary pull-right" ui:field="btnDone"
					text="DONE" />
				<g:Button styleName="btn btn-default pull-left" ui:field="btnCancel"
					text="CANCEL" />
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder>
