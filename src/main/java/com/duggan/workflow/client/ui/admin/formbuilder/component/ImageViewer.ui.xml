<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:up='urn:import:com.duggan.workflow.client.ui.upload.custom'>
	<g:HTMLPanel>
		<div class="form-group">
			<g:FocusPanel ui:field="uploadPanel">
				<up:Uploader ui:field="uploader" styleName="custom-file-input hide"></up:Uploader>
			</g:FocusPanel>

			<g:FocusPanel ui:field="panelPicture">
				<g:HTMLPanel styleName="form-group">
					<!-- <g:Image ui:field="img" width="70px" height="90px" /> -->
					<g:Image ui:field="img" width="179px" />
				</g:HTMLPanel>
			</g:FocusPanel>

			<div class="form-group" ui:field="divGroups">
			</div>

		</div>

	</g:HTMLPanel>
</ui:UiBinder> 