<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="tr">
						<g:HTMLPanel styleName="center td">
							<label>
								<input type="checkbox"/>
							</label>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnRDBMSName"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnConfigName"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnJNDIName"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnDriver"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnURL"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnUser"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnStatus"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnLastModified"></span>
						</g:HTMLPanel>
						
						<g:HTMLPanel styleName="td">
								<g:Anchor ui:field="aTest" styleName="btn btn-default" 
								    title="Test data source">
									<i class="glyphicon glyphicon-play"></i>
								</g:Anchor>
								
								<g:Anchor ui:field="aEdit" styleName="btn btn-default"
								     title="Edit Data Source">
									<i class="glyphicon glyphicon-edit"></i>
								</g:Anchor>
								<g:Anchor ui:field="aDelete" styleName="btn btn-default"
								     title="Delete Data Source">
									<i class="glyphicon glyphicon-trash"></i>
								</g:Anchor>
						</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>
