<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v='urn:import:com.duggan.workflow.client.ui.component'>
	<g:HTMLPanel>
		<v:IssuesPanel ui:field="issues">
		</v:IssuesPanel>
		<div class="row item">
			<div class="col-md-12">
				<legend>
				    <span ui:field="spnFormCaption"></span>
					<div style="float:right; text-align:right;font-weight: initial;">
						<span ui:field="spnCreated" class="tasktimelines"></span>
						<span ui:field="spnDeadline" class="tasktimelines"></span>
					</div>
				</legend>
			</div>
		</div>
		<div class="row item">
			<span ui:field="divFormHelp" styleName="text-info"></span>
		</div>
		<g:HTMLPanel styleName="form-horizontal" ui:field="panelFields">

		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>