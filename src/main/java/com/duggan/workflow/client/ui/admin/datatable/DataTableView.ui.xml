<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:d='urn:import:com.duggan.workflow.client.ui.component'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<div class="content-body">
			<div class="row-fluid">
				<div class="action-buttons">
					<g:Anchor ui:field="aNew" styleName="btn btn-primary"
						title="New Form">
						<i class="icon-plus"></i>
						New Table
					</g:Anchor>
					<g:Anchor ui:field="aNewReport" styleName="btn btn-primary"
						title="New Form">
						<i class="icon-plus"></i>
						New Report Table
					</g:Anchor>
					<g:Anchor ui:field="aNewReportView" styleName="btn btn-primary"
						title="New Form">
						<i class="icon-plus"></i>
						New Report View
					</g:Anchor>
					<g:Anchor ui:field="aImport" styleName="btn btn-primary"
						title="Import Form">
						<i class="icon-upload-alt"></i>
						Import Table
					</g:Anchor>
					<g:Anchor ui:field="aExport" styleName="btn btn-primary"
						title="Export Form">
						<i class="icon-arrow-down"></i>
						Export Table
					</g:Anchor>
				</div>
				
				<d:TableView ui:field="tblView"></d:TableView>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
