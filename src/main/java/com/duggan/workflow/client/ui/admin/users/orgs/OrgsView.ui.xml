<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
             
    	<g:HTMLPanel styleName="tr">
			<g:HTMLPanel styleName="td">
				<label>
					<input type="checkbox"/>
				</label>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="panelName" styleName="td"></g:HTMLPanel>
			<g:HTMLPanel ui:field="panelFullName" styleName="td"></g:HTMLPanel>
			<g:HTMLPanel styleName="td">
				<div class="action-commands">										
					<g:Anchor styleName="green" ui:field="aEdit">
						<i class="icon-pencil helper-14"></i>
					</g:Anchor>
					<g:Anchor styleName="red" ui:field="aDelete">
						<i class="icon-trash"></i>
					</g:Anchor>
				</div>
		</g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>