ldap.bind.pwd=secret
ldap.user.ctx=ou=users,o=mojo
ldap.role.ctx=ou=groups,o=mojo
ldap.user.roles.ctx=ou=groups,o=mojo
ldap.user.filter=(uid={0})
ldap.role.filter=(cn={0})
ldap.user.roles.filter=(&(objectClass=group)(uniqueMember=cn={0},ou=users,o=mojo))
ldap.user.id.dn=true
