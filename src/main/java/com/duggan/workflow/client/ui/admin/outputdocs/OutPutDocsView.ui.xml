<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	xmlns:d='urn:import:com.duggan.workflow.client.ui.component' ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<div class="content-body overflow-y">
			<div class="row-fluid">
				<div class="action-buttons">
					<g:Anchor ui:field="aNewDocument" styleName="btn btn-primary"
						title="Add a New Process">
						<i class="icon-plus"></i>
						Add Document
					</g:Anchor>
				</div>
			</div>

			<g:FlexTable ui:field="tblView" styleName="table-programs">
			</g:FlexTable>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
