<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d='urn:import:com.duggan.workflow.client.ui.component'>
	<g:HTMLPanel styleName="control-group" ui:field="panelGroup">
		<label ui:field="lblEl" class="control-label">List Field:</label>
		<g:HTMLPanel styleName="controls" ui:field="panelControls">
			<d:DropDownList ui:field="lstItems" styleName="input-xlarge" title=""></d:DropDownList>
			<g:InlineLabel ui:field="lblComponent" styleName="help-inline"></g:InlineLabel>
			<span ui:field="spnMsg" class="hide help-inline helper-font-14">
					<i class="icon" ui:field="spnIcon"></i>
				</span>
			<span ui:field="spnMandatory" style="color:red">*</span>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 