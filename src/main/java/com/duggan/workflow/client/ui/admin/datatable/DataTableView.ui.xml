<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<div class="content-body">
			<div class="row-fluid">
				<div class="action-buttons">
					<g:Anchor ui:field="aNew" styleName="btn btn-primary"
						title="New Form">
						<i class="icon-plus"></i>
						New Table
					</g:Anchor>
					<g:Anchor ui:field="aImport" styleName="btn btn-primary"
						title="Import Form">
						<i class="icon-upload-alt"></i>
						Import Table
					</g:Anchor>
					<g:Anchor ui:field="aData" styleName="btn btn-primary"
						title="View/Edit Data">
						<i class="icon-hdd"></i>
						Data View
					</g:Anchor>
					<g:Anchor ui:field="aExport" styleName="btn btn-primary"
						title="Export Form">
						<i class="icon-arrow-down"></i>
						Export Table
					</g:Anchor>
					<g:Anchor ui:field="aDelete" styleName="btn btn-danger"
						title="Delete Form">
						<i class="icon-trash"></i>
						Delete Table
					</g:Anchor>
				</div>

				<div class="table table-striped table-hover">
					<div class="thead">
						<div class="tr">
							<div class="th">
								<label>
									<input type="checkbox" class="ace" />
								</label>
							</div>
							<div class="th">Name</div>
							<div class="th">Description</div>
							<div class="th">Records</div>
							<div class="th">LAST MODIFIED</div>
							<div class="th">ACTIONS</div>
						</div>
					</div>
					<g:HTMLPanel ui:field="tblRow" styleName="tbody">
						<div class="tr">
							<div class="td">
								<label>
									<input type="checkbox" class="ace" />
								</label>
							</div>
							<div class="td">SUPPLIERS</div>
							<div class="td">List of Vendors and their bank details</div>
							<div class="td">50</div>
							<div class="td">02/10/2015</div>
							<div class="td">
								<div class="action-commands">
									<g:Anchor styleName="btn btn-success" ui:field="aEdit">
										<i class="icon-pencil helper-14"></i>
									</g:Anchor>
									<g:Anchor styleName="btn btn-danger" ui:field="aDel">
										<i class="icon-trash"></i>
									</g:Anchor>
								</div>
							</div>
						</div>
						<div class="tr">
							<div class="td">
								<label>
									<input type="checkbox" class="ace" />
								</label>
							</div>
							<div class="td">Accounts</div>
							<div class="td">Chart of Accounts</div>
							<div class="td">200</div>
							<div class="td">02/10/2015</div>
							<div class="td">
								<div class="action-commands">
									<g:Anchor styleName="btn btn-success" ui:field="aEdit2">
										<i class="icon-pencil helper-14"></i>
									</g:Anchor>
									<g:Anchor styleName="btn btn-danger" ui:field="aDel2">
										<i class="icon-trash"></i>
									</g:Anchor>
								</div>
							</div>
						</div>
					</g:HTMLPanel>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
